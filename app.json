{
  "expo": {
    "name": "VisionBridge Mobile",
    "slug": "visionbridge-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "visionbridge",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-video",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
